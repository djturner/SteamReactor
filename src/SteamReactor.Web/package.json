        "prop-types": "^15.6.2",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-redux": "^5.1.1",
        "redux": "^4.0.1",
        "redux-saga": "^0.16.2",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-loader": "^7",
        "babel-plugin-es6-promise": "^1.1.1",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "css-loader": "^1.0.1",
        "eslint": "^5.9.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
