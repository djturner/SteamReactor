{
    "name": "steam-reactor",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build:watch": "npm run prebuild && webpack --config=webpack.config.dev.js --watch=true",
        "release-build": "webpack --watch=true",
        "clean-wwwroot": "npm run remove-wwwroot && mkdir wwwroot",
